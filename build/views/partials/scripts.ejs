<script>
  $(function () {
    var socket = io(); 
    var bank = Cookies.get('userBank');

    socket.on('addNew', function(msg) {
      console.log('client ', msg, bank);
      if(msg != bank)
        setTimeout(function () {
          window.location.href= '/transfers';
        }, 2000)
        
      console.log('addNew client = ', {msg});
    });
  });
</script>